namespace Inriver.AccessControl.Policies.Log
{
    import Oasis.Attributes

    attribute Subject
    {
        id = "log.subject"
        type = string
        category = subjectCat
    }

    attribute Message
    {
        id = "log.message"
        type = string
        category = subjectCat
    }

    attribute DateTime
    {
        id = "log.dateTime"
        type = dateTime
        category = subjectCat
    }

    obligation Log = "Log"
}